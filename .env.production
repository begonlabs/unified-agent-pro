# Configuración de Producción para OndAI Frontend
# ⚠️ IMPORTANTE: Estos valores deben ser configurados en tu VPS

# Environment
NODE_ENV=production

# Supabase Configuration (Actualizar con tus valores reales)
VITE_SUPABASE_URL=http://tu-vps-ip:8000
VITE_SUPABASE_ANON_KEY=tu_anon_key_aqui
SERVICE_ROLE_KEY=tu_service_role_key_aqui

# Email Configuration
ENABLE_EMAIL_AUTOCONFIRM=true

# Security
CORS_ORIGINS=https://tu-dominio.com,https://www.tu-dominio.com

# Performance
VITE_APP_CACHE_VERSION=1.0.0

# Analytics (opcional)
VITE_GA_TRACKING_ID=
VITE_HOTJAR_ID=

# Error Tracking (opcional)
VITE_SENTRY_DSN=

# App Configuration
VITE_APP_NAME=OndAI
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION="Plataforma de comunicación empresarial con IA"